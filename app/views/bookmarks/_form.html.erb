<%= form_for bookmark do |f| %>
     <%= f.label :title %> <br>
     <%= f.text_field :title %>
     <br>
     <%= f.label :category %> <br>
     <%= f.text_area :category %>
     <br>
     <%= f.label :url %> <br>
     <%= f.text_field :url %>
     <br>
     <%= f.label :is_favorite %> <br>
     <%= f.text_field :is_favorite %>
     <br>
     <%= f.label :comment %> <br>
     <%= f.text_field :comment %>
     <br>
     <%= f.submit %>
<% end %>
